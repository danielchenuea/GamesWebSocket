{
  "name": "diretorio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "build": "webpack --mode=development"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "verbose": true,
    "testRegex": "(/Tests/.*|(\\.|/)(test|spec))\\.tsx$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node",
      "tsx"
    ],
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "setupFiles": [
      "<rootDir>/setup.jest.js"
    ],
    "extensionsToTreatAsEsm": [
      ".tsx",
      ".ts"
    ],
    "transform": {
      "\\.[t]sx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/datatables.net": "^1.10.28",
    "@types/jest": "^29.5.11",
    "@types/jquery": "^3.5.29",
    "@types/jquery-loading-overlay": "^1.0.4",
    "@types/select2": "^4.0.63",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-editor-support": "^31.1.2",
    "jest-environment-jsdom": "^29.7.0",
    "jquery": "^3.7.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@types/signalr": "^2.4.3"
  }
}
